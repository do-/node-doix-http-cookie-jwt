{
  "name": "doix-http-cookie-jwt",
  "version": "1.0.1",
  "description": "doix http cookie based sessions using jwt",
  "main": "index.js",
  "files": ["/lib", "/badges"],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci --coverage"
  },
  "jest": {
	"collectCoverageFrom": [
		"lib/**/*.js"
	],
	"coverageReporters": ["clover", "json", "lcov", "text", "json-summary"],
  	"verbose": false,
  	"testPathIgnorePatterns": [
  		"/node_modules/",
  		"/__tests__/data",
  		"/__tests__/lib"
  	]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/do-/node-doix-http-cookie-jwt.git"
  },
  "keywords": [
    "doix",
    "http",
    "cookie",
    "session",
    "jwt"
  ],
  "author": "Dmitry Ovsyanko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/do-/node-doix-http-cookie-jwt/issues"
  },
  "homepage": "https://github.com/do-/node-doix-http-cookie-jwt#readme",
  "peerDependencies": {
    "doix-http": "^1.0.2"
  },
  "devDependencies": {
    "jest": "^29.3.1"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2"
  }
}
